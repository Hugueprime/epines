{
    "name": "Epines",
    "description": "L'extension qui vous enlève les épines du pied (et des sites de l'epita)!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches":[
                "https://intra.assistants.epita.fr/*",
                "https://ionisx.com/*",
                "https://courses.ionisx.com/*"
            ],
            "js": [
                "/contents/tools.js",
                "/contents/intraAssistants.js",
                "/contents/ionisx.js",
                "/contents/mainContent.js"
            ],
            "css":[
                "/styles/globalAdded.css",
                "/styles/ionisxAdded.css",
                "/styles/ionisx.css"
            ]

        }
    ]
}